            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "escape",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "escape"
                  }
                ],
                "to_after_key_up": [
                  {
                    "select_input_source": {
                      "input_source_id": "com.apple.keylayout.UnicodeHexInput"
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                      "^com\\.apple\\.Terminal$",
                      "^com\\.googlecode\\.iterm2$",
                      "^com\\.jetbrains\\.goland$",
                      "^com\\.jetbrains\\.intellij$",
                      "^com\\.jetbrains\\.intellij\\.ce$",
                      "^md\\.obsidian$",
                      "^dev\\.zed\\.Zed$"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "description": "capslock to switch profile",
            "manipulators": [
              {
                "from": { "key_code": "caps_lock" },
                "to": [
                  {
                    "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'Function Mode'"
                  }
                ],
                "type": "basic"
              }
            ]
          }
